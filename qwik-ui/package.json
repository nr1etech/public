{
  "name": "@nr1e/qwik-ui",
  "version": "0.0.32",
  "description": "NR1E Qwik UI Library",
  "author": "NR1E, Inc.",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nr1etech/public.git"
  },
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    },
    "./styles.css": "./styles.css"
  },
  "files": [
    "lib",
    "lib-types",
    "styles.css"
  ],
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild": "prettier --check . && eslint .",
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf lib lib-types",
    "dev": "vite --mode ssr --port 5175",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "start": "vite --open --mode ssr",
    "test": "vitest run src",
    "qwik": "qwik"
  },
  "peerDependencies": {
    "@builder.io/qwik-city": "catalog:",
    "@nr1e/qwik-icons": "workspace:"
  },
  "devDependencies": {
    "@builder.io/qwik": "catalog:",
    "@eslint/js": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@types/node": "catalog:",
    "daisyui": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-qwik": "catalog:",
    "globals": "catalog:",
    "np": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "undici": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  }
}
