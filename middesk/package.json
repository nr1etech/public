{
  "name": "@nr1e/middesk",
  "description": "Middesk client",
  "version": "0.0.1",
  "type": "module",
  "author": "NR1E, Inc.",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nr1etech/lib-js.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "prettier --check . && eslint .",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -w",
    "test": "vitest run -t @unit src",
    "test:int": "dotenv -- vitest run -t @int src",
    "test:only": "dotenv -- vitest run -t @only src",
    "clean": "rm -rf dist",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "@types/uuid": "catalog:",
    "dotenv-cli": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vitest": "catalog:"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./businesses": {
      "import": "./dist/businesses/index.mjs",
      "types": "./dist/businesses/index.d.mts"
    },
    "./orders": {
      "import": "./dist/orders/index.mjs",
      "types": "./dist/orders/index.d.mts"
    },
    "./liens": {
      "import": "./dist/liens/index.mjs",
      "types": "./dist/liens/index.d.mts"
    },
    "./policy-results": {
      "import": "./dist/policy-results/index.mjs",
      "types": "./dist/policy-results/index.d.mts"
    },
    "./reviews": {
      "import": "./dist/reviews/index.mjs",
      "types": "./dist/reviews/index.d.mts"
    },
    "./tin-match": {
      "import": "./dist/tin-match/index.mjs",
      "types": "./dist/tin-match/index.d.mts"
    },
    "./websites": {
      "import": "./dist/websites/index.mjs",
      "types": "./dist/websites/index.d.mts"
    },
    "./webhooks": {
      "import": "./dist/webhooks/index.mjs",
      "types": "./dist/webhooks/index.d.mts"
    }
  }
}
