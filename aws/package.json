{
  "name": "@nr1e/aws",
  "description": "Provides common AWS utilities",
  "version": "0.0.0",
  "type": "module",
  "author": "NR1E, Inc.",
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nr1etech/lib-js.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "prettier --check . && eslint .",
